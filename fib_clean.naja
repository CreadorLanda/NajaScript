fun fibonacci(int n) {
    if (n <= 1) {
        return n;
    }
    
    int a = 0;
    int b = 1;
    int i = 2;
    int c = 0;
    
    while (i <= n) {
        c = a + b;
        a = b;
        b = c;
        i = i + 1;
    }
    
    return b;
}

println("Calculando Fibonacci(20)...");
int resultado = fibonacci(20);
println("Fibonacci(20) = " + resultado);

