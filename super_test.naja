// Exemplo simples para testar super() com o argument_list

// Classe base
class Animal {
    string nome;
    
    constructor(string nome) {
        this.nome = nome;
    }
    
    public void fazerSom() {
        println(this.nome + " faz um som genérico.");
    }
}

// Classe derivada que usa super()
class Cachorro extends Animal {
    string raca;
    
    constructor(string nome, string raca) {
        super(nome); // Testando super com um argumento
        this.raca = raca;
    }
    
    public void fazerSom() {
        println(this.nome + " da raça " + this.raca + " faz AU AU!");
    }
}

// Programa principal
fun main() {
    println("=== Teste de super() ===");
    
    Animal animal = new Animal("Bicho");
    animal.fazerSom();
    
    Cachorro cachorro = new Cachorro("Rex", "Labrador");
    cachorro.fazerSom();
    
    println("=== Fim do teste ===");
}

// Executar o programa
main(); 