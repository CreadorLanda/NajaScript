{
  "$schema": "vscode://schemas/color-theme",
  "name": "NajaScript Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#1E1E1E",
    "editor.foreground": "#D4D4D4",
    "editor.lineHighlightBackground": "#2D2D2D",
    "editor.selectionBackground": "#264F78",
    "editor.selectionHighlightBackground": "#264F7880",
    "editorCursor.foreground": "#FFFFFF",
    "editorWhitespace.foreground": "#3B3B3B",
    "editorIndentGuide.background": "#404040",
    "editorIndentGuide.activeBackground": "#707070",
    "editor.findMatchBackground": "#515C6A",
    "editor.findMatchHighlightBackground": "#EA5C0055",
    "editorLineNumber.foreground": "#858585",
    "editorLineNumber.activeForeground": "#C6C6C6",
    "editorBracketMatch.background": "#0064001A",
    "editorBracketMatch.border": "#888888",
    "debugToolBar.background": "#333333",
    "activityBar.background": "#333333",
    "sideBar.background": "#252526"
  },
  "tokenColors": [
    {
      "scope": ["variable", "variable.other", "variable.other.readwrite"],
      "settings": {
        "foreground": "#9CDCFE"
      }
    },
    {
      "scope": ["variable.other.constant", "variable.other.constant.property"],
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "entity.name.function.method",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#DCDCAA"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#9CDCFE"
      }
    },
    {
      "scope": ["entity.name.type.class", "entity.name.class", "support.class"],
      "settings": {
        "foreground": "#4EC9B0"
      }
    },
    {
      "scope": ["variable.language.this", "variable.language.this.js"],
      "settings": {
        "foreground": "#C586C0"
      }
    },
    {
      "scope": ["variable.language.super", "support.variable.property.super"],
      "settings": {
        "foreground": "#C586C0"
      }
    },
    {
      "scope": ["keyword.operator.new"],
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "scope": ["storage.modifier", "keyword.other", "storage.modifier.extends"],
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "scope": ["support.type", "entity.name.type", "entity.name.type.interface"],
      "settings": {
        "foreground": "#4EC9B0"
      }
    },
    {
      "scope": ["string.quoted", "string.template", "string.quoted.module"],
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#B5CEA8"
      }
    },
    {
      "scope": ["constant.language.boolean.true", "constant.language.boolean.false"],
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "scope": ["constant.language.null", "constant.language.undefined"],
      "settings": {
        "foreground": "#C586C0"
      }
    },
    {
      "scope": ["keyword.control", "keyword.control.import"],
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "scope": ["comment", "comment.line", "comment.block"],
      "settings": {
        "foreground": "#6A9955"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "scope": ["support.module", "support.type"],
      "settings": {
        "foreground": "#4EC9B0"
      }
    },
    {
      "scope": ["variable.other.property"],
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "scope": ["storage.type", "keyword", "storage.type.function"],
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.embedded",
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "scope": "meta.preprocessor",
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "scope": "meta.preprocessor.string",
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "scope": "meta.preprocessor.numeric",
      "settings": {
        "foreground": "#B5CEA8"
      }
    },
    {
      "scope": "meta.structure.dictionary.key",
      "settings": {
        "foreground": "#9CDCFE"
      }
    },
    {
      "scope": "storage",
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#D7BA7D"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796E6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#CD9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#F44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#B267E6"
      }
    }
  ]
} 