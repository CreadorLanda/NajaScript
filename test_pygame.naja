// Simple test file for NajaScript with Pygame
import "Pygame";

// Initialize the game
initGame(800, 600, "NajaScript Test");

// Main game loop
while (updateWindow()) {
    // Clear the screen
    clearScreen(0, 0, 0);
    
    // Draw some text
    drawText("NajaScript Pygame Test", 300, 280, 24, 255, 255, 255);
    
    // Draw a rectangle
    drawRect(350, 320, 100, 50, 255, 0, 0);
}

// Quit the game
quitGame(); 