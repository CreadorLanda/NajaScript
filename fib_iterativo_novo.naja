// Implementacao iterativa de Fibonacci
fun fibonacci(int n) {
    if (n <= 1) {
        return n;
    }
    
    int a = 0;
    int b = 1;
    int resultado = 0;
    
    int i = 2;
    while (i <= n) {
        resultado = a + b;
        a = b;
        b = resultado;
        i = i + 1;
    }
    
    return resultado;
}

println("Calculando Fibonacci(20) de forma iterativa...");
int resultado = fibonacci(20);
println("Fibonacci(20) = " + resultado);

println("Programa concluido!"); 