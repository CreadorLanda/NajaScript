// Test file for NajaScript interpreter

// Test variables and primitive types
int a = 5;
int b = 4;
print("The sum of " + a + " and " + b + " is " + (a+b) + "\n");

// Test function
fun soma(int x, int y) : int {
    return x + y;
}

print("Function call result: " + soma(a, b) + "\n");

// Test simple assignment
a = a + b;
