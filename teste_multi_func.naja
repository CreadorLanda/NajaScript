// Script para testar múltiplas funções

// Função de cálculo
fun dobrar(n) {
    return n * 2;
}

// Função principal
fun main() {
    println("Teste de múltiplas funções");
    
    int num = 5;
    println("O dobro de " + num + " é: " + dobrar(num));
    
    // Teste com outro valor
    println("O dobro de 10 é: " + dobrar(10));
}

// Chamada da função principal
main(); 