// Exemplo simples de operadores lógicos
importar "NajaPt";

funcao principal() {
    // Operadores AND e OR
    booleano a = verdadeiro;
    booleano b = falso;
    
    escreverln("a = verdadeiro, b = falso");
    escreverln("a && b = " + (a && b));
    escreverln("a || b = " + (a || b));
    
    // Operador NOT - usando comparação com falso/verdadeiro
    booleano resultado = (b == falso);  // Equivalente a !b
    escreverln("Negação de b (b == falso): " + resultado);
}

principal(); 
