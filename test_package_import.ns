import "test_package";

fun main() {
    println("=== Testando pacote test_package ===");
    
    // Verificar o que está disponível no pacote
    println("Verificando conteúdo disponível no pacote:");
    
    // Testar constantes exportadas
    println("PI disponível: " + (test_package.PI != null));
    println("PI = " + test_package.PI);
    
